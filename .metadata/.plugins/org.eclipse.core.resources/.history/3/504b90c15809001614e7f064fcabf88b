package ie.dit;

public class LinkedList {
	
	public Link firstLink;
	
	LinkedList() {
		firstLink = null;
	}
	
	public void insertLink(int value) {
		Link newLink = new Link(value);
		newLink.next = firstLink;
		firstLink = newLink;
		
	}
	
	public boolean isEmpty() {
		return(firstLink == null);
	}
	
	

}
