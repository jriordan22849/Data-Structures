package ie.dit;

public class Link {
	int value;
	
	public Link next;
	
	
	public Link(int value) {
		this.value = value;
	}
	
	public void display() {
		System.out.println(value);
	}
	
	public static void main(String[] args) {
		LinkedList list = new LinkedList();
		for(int i = 0; i < 5; i ++) {
			list.insertLink(i);
		}
		list.display();
		
	}
	
	
}
